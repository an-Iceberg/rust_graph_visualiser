{
  "configurations": [
    {
      "name": "Check",
      "type": "f5anything",
      "request": "launch",
      "command": "cargo check --verbose --color always --message-format human",
      "terminalName": "Cargo",
      "showTerminal": true
    },
    {
      "name": "Clean",
      "type": "f5anything",
      "request": "launch",
      "command": "cargo clean",
      "terminalName": "Cargo",
      "showTerminal": true
    },
    {
      "name": "Fetch",
      "type": "f5anything",
      "request": "launch",
      "command": "cargo fetch",
      "terminalName": "Cargo",
      "showTerminal": true
    },
    {
      "name": "Test",
      "type": "f5anything",
      "request": "launch",
      "command": "cargo test --verbose --color always --message-format human",
      "terminalName": "Cargo",
      "showTerminal": true,
    },
    {
      "name": "Run",
      "type": "f5anything",
      "request": "launch",
      "command": "cargo run --verbose --color always --message-format human",
      "terminalName": "Cargo",
      "showTerminal": true
    },
    {
      "name": "Debug",
      "type": "lldb",
      "request": "launch",
      "program": "${workspaceRoot}/target/debug/${workspaceFolderBasename}.exe",
      "args": [],
      "cwd": "${workspaceFolder}",
      "sourceLanguages": [
        "rust"
      ]
    },
    {
      "name": "Build",
      "type": "f5anything",
      "request": "launch",
      "command": "cargo build --verbose --color always --message-format human",
      "terminalName": "Cargo",
      "showTerminal": true
    },
    {
      "name": "Build WASM",
      "type": "f5anything",
      "request": "launch",
      "command": "cargo build --verbose --color always --message-format human --target wasm32-unknown-unknown",
      "terminalName": "Cargo",
      "showTerminal": true
    },
    {
      "name": "Run release",
      "type": "f5anything",
      "request": "launch",
      "command": "cargo run --release --verbose --color always --message-format human",
      "terminalName": "Cargo",
      "showTerminal": true
    },
    {
      "name": "Build release",
      "type": "f5anything",
      "request": "launch",
      "command": "cargo build --release --verbose --color always --message-format human",
      "terminalName": "Cargo",
      "showTerminal": true
    },
    {
      "name": "Build release WASM",
      "type": "f5anything",
      "request": "launch",
      "command": "cargo build --release --verbose --color always --message-format human --target wasm32-unknown-unknown",
      "terminalName": "Cargo",
      "showTerminal": true
    },
    {
      "name": "Build release for all Platforms",
      "type": "f5anything",
      "request": "launch",
      "command": "cargo build --release --target wasm32-unknown-unknown; cargo build --release --target x86_64-pc-windows-msvc; cargo build --release --target x86_64-apple-darwin; cargo build --release --target x86_64-unknown-linux-gnu",
      "terminalName": "Cargo",
      "showTerminal": true
    },
    {
      "name": "Vite",
      "type": "f5anything",
      "request": "launch",
      "command": "vite ${workspaceFolder}/docs",
      "terminalName": "Vite",
      "showTerminal": true
    },
  ]
}
